    "electron-is-dev": "2.0.0",
    "eslint-config-airbnb": "18.2.1",
    "expect.js": "0.3.1",
    "fabric": "4.3.1",
    "file-saver": "2.0.5",
    "firebase": "^8.5.0",
    "gapi-script": "1.1.0",
