    "zlib": "1.0.5"
  },
  "scripts": {
    "start": "node ./src/GridaBoard/util/exceltojson.js && react-scripts start",
    "https": "set HTTPS=true&&react-scripts start",
    "build": "set GENERATE_SOURCEMAP=true && react-scripts build",
    "test": "react-scripts test",
    "react-eject": "react-scripts eject",
    "react-start": "react-scripts start",
    "e:start": "concurrently \"cross-env NODE_ENV=development BROWSER=none npm run react-start\" \"wait-on http://localhost:3000 && electron .\"",
    "e:build": "npm run build && electron-builder",
    "test-deploy": "node ./src/GridaBoard/util/exceltojson.js && npm run build && firebase use test && firebase deploy",
    "deploy": "node ./src/GridaBoard/util/exceltojson.js && npm run build && firebase use live && firebase deploy"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "airbnb",
      "eslint:recommended",
      "plugin:react/recommended",
      "plugin:@typescript-eslint/recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "plugins": [
      "react",
      "@typescript-eslint"
    ],
    "rules": {}
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "rules": {
    "no-unused-vars": "off",
    "no-prototype-builtins": "off",
    " @typescript-eslint/no-inferrable-types": "off"
  },
  "description": "GridaBoard by Neo smartpen",
  "author": "NeoLAB Convergence",
  "build": {
    "asar": true,
    "appId": "com.neosmartpen.gridaboard",
    "productName": "GridaBoard2",
    "protocols": {
      "name": "gridaboard",
      "schemes": [
        "gridaboard"
      ]
    },
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64"
          ]
        }
      ]
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true,
      "perMachine": false,
      "createDesktopShortcut": true
    }
  },
  "main": "public/electron.js",
  "homepage": "./",
  "devDependencies": {
    "@types/brainhubeu__react-carousel": "^2.0.2",
    "@types/fabric": "4.2.3",
    "@types/jest": "26.0.22",
    "@types/node": "14.14.37",
    "@types/pdfjs-dist": "2.1.6",
    "@types/react-redux": "7.1.16",
    "@types/sprintf-js": "1.1.2",
    "@types/web-bluetooth": "0.0.4",
    "@typescript-eslint/eslint-plugin": "4.20.0",
    "@typescript-eslint/parser": "4.20.0",
    "concurrently": "6.0.0",
    "cross-env": "7.0.3",
    "electron": "12.0.2",
    "electron-builder": "22.10.5",
    "eslint": "7.23.0",
    "eslint-plugin-css-modules": "2.11.0",
    "eslint-plugin-react": "7.23.1",
    "eslint-plugin-react-hooks": "^4.3.0",
    "typescript": "4.2.3",
    "wait-on": "5.3.0"
  }
}
